{
  "source": {
    "type": "elasticsearch",
    "params": {
      "host": "host:port",
      "username": "username",
      "password": "password",
      "index": "you-index-pattern-*",
      "query": {
        "match": {
          "username": "blaklaybul"
        }
      }
    }
  },
  "dest":{
    "type": "filesystem",
    "params": {
      "directory": "output"
    }
  },
  "include": {
    "field.1" : "file_path",
    "field.2": "ipv4",
    "@timestamp": null
  },
  "exclude": [],
  "include_rest": false
}