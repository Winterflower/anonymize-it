{
  "source": {
    "type": "elasticsearch",
    "params": {
      "host": "host:port",
      "username": "username",
      "password": "password",
      "index": "index*"
    }
  },
  "dest":{
    "type": "filesystem",
    "params": {
      "directory": "path/to/directory"
    }
  },
  "masks": {
    "field.name.1" : "file_path",
    "field.name.2": "ipv4"
  },
  "exclude": ["fields", "to", "suppress"],
  "include_all": false
}